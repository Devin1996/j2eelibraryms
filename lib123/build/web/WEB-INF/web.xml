<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>libregister</servlet-name>
        <servlet-class>Servlets.libregister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginservlert</servlet-name>
        <servlet-class>Servlets.loginservlert</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addservlet</servlet-name>
        <servlet-class>Servlets.addservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adddao</servlet-name>
        <servlet-class>DAO.adddao</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchservlet</servlet-name>
        <servlet-class>Servlets.searchservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>locationservlet</servlet-name>
        <servlet-class>Servlets.locationservlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>libregister</servlet-name>
        <url-pattern>/libregister</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginservlert</servlet-name>
        <url-pattern>/loginservlert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addservlet</servlet-name>
        <url-pattern>/addservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adddao</servlet-name>
        <url-pattern>/adddao</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>searchservlet</servlet-name>
        <url-pattern>/searchservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>locationservlet</servlet-name>
        <url-pattern>/locationservlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
